{
    "functions": {
        "dependencies": {
            "include": ["requirements.txt"],
            "postInstallCommand": "pip install -r requirements.txt"
        }
    }
    "builds": [{
        "src": "P5/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles_build" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "P5/wsgi.py"
        }
    ]
}
