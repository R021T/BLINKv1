{
  "builds": [
    {
      "src": "P5/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      },
      "env": {
        "CMAKE_BINARY_URL": "https://cmake.org/files/v3.22/cmake-3.22.1-Linux-x86_64.sh"
      },
      "postBuild": "sh ./vercel/post-build.sh"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "P5/wsgi.py"
    }
  ]
}
