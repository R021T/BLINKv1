{
    "builds": [{
        "src": "P5/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles_build" }
    },
        "dependencies": {
            "mysql": "^2.18.1",
            "mariadb": "^2.5.3"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "P5/wsgi.py"
        }
    ]
}
